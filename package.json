{
  "name": "mytodo",
  "displayName": "mytodo",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:mytodo-sidebar"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mytodo.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "mytodo.openSidebar",
        "title": "Open MyToDo Sidebar"
      },
      {
        "command": "mytodo.openAPIPanel",
        "title": "Get today fixtures"
      },
      {
        "command": "mytodo.addTodoItem",
        "title": "Add Todo Item"
      },
      {
        "command": "mytodo.openNewsPanel",
        "title": "Get football news"
      },
      {
        "command": "mytodo.openTransferNewsPanel",
        "title": "Get transfer updates"
      },
      {
        "command": "mytodo.openMatchResultsPanel",
        "title": "Get scoreboard"
      },
      {
        "command": "mytodo.openCricketMatchPanel",
        "title": "Get live cricket "
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.87.0",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.8"
  }
}
